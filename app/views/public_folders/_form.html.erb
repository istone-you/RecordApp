<%= form_with(model: @public_folder, local: true) do |form| %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  
  <div class="field">
    <%= form.hidden_field :image %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>


  
